  "version": "0.2.0",
  "configurations": [
    {
      "name": "RST (Debug Mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "debug",
      "args": [
        "--dart-define-from-file=.dev.env", 
        "--web-port=5055"
      ]
    },
    {
      "name": "RST (Profile Mode)",
      "type": "dart",
      "request": "launch",
      "program": "lib/main.dart",
      "flutterMode": "profile",
      "args": [
        "--dart-define-from-file=.env", 
        "--web-port=5055"
      ]
    }
  ]
}